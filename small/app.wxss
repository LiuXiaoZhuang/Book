page{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: stretch;
}
.main_box{
  width: 100%;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
}

@font-face {font-family: "book_font";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

[class^="book-icon"], [class*=" book-icon"] {
  font-family:"book_font" !important;
  font-size:32rpx;
  speak:none;
  font-style:normal;
  font-weight:400;
  font-variant:normal;
  text-transform:none;
  line-height:1;
  vertical-align:baseline;
  display:inline-block;
  -webkit-font-smoothing:antialiased;
  -moz-osx-font-smoothing:grayscale;
}

.book-icon-sum:before { content: "\e632"; }
.book-icon-moon:before { content: "\e633"; }
.book-icon-settings:before { content: "\e634"; }
.book-icon-right:before { content: "\e7eb"; }
.book-icon-left:before { content: "\e7ec"; }
.book-icon-menu:before { content: "\e7f4"; }